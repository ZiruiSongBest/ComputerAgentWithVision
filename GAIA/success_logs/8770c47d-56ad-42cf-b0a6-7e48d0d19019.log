from friday.action.base_action import BaseAction
import re
import os

class extract_forest_area_data(BaseAction):
    def __init__(self):
        self._description = "Extract the forest area data for specific countries and years from a converted text file."

    def __call__(self, file_path, countries, year, working_dir=None):
        """
        Extracts forest area data for specified countries and a specific year from a text file.

        Args:
            file_path (str): The path to the text file containing the data.
            countries (list): A list of countries for which the data is to be extracted.
            year (str): The year for which the data is to be extracted.
            working_dir (str, optional): The working directory. If not provided, uses the current working directory.

        Returns:
            dict: A dictionary with countries as keys and their respective forest area data for the specified year as values.
        """
        if working_dir:
            os.chdir(working_dir)
        else:
            working_dir = os.getcwd()

        # Ensure the file path is absolute
        if not os.path.isabs(file_path):
            file_path = os.path.join(working_dir, file_path)

        try:
            with open(file_path, 'r', encoding='utf-8') as file:
                text = file.read()

            data = {}
            for country in countries:
                # Regex pattern to find the forest area data for the specified country and year
                pattern = rf"{country}.*?{year}.*?(\d+\.?\d*)"
                match = re.search(pattern, text, re.DOTALL)
                if match:
                    data[country] = match.group(1)
                else:
                    data[country] = "Data not found"

            print("Forest area data extraction completed.")
            return data
        except FileNotFoundError:
            print("The specified file was not found.")
        except Exception as e:
            print(f"An error occurred: {e}")


